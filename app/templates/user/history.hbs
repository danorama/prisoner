<div class="row">
  <div class="col-md-12">
    <h2>History</h2>
    <table class="table table-striped table-condensed">
      <thead>
        <tr>
          <th>Period</th>
          <th>Stage</th>
          <th>You</th>
          <th>Other</th>
          <th>Outcome</th>
          <th>Earnings</th>
          <th>Group</th>
        </tr>
      </thead>
      <tbody>
        {{#each item in history}}
        <tr>
          <td>{{item.period}}</td>
          <td>{{item.stage}}</td>
          <td>{{item.you}}</td>
          <td>{{item.other}}</td>
          <td>X, X</td>
          <td>10</td>
          <td></td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>
